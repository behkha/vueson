  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Behzad Khanlar",
  "repository": {
    "type": "git",
    "url": "https://github.com/behkha/vueson.git"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "sortablejs": "^1.15.6"
  }
