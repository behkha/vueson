{
  "name": "vueson",
  "version": "1.0.0",
  "description": "A visual JSON schema builder for Vue 3 using TailwindCSS and shadcn-vue components.",
  "type": "module",
  "main": "dist/vueson.umd.js",
  "module": "dist/vueson.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "vite build && gh-pages -d dist"
  },
  "peerDependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.548.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "json",
    "schema",
    "builder",
    "tailwind",
    "shadcn",
    "ui",
    "visual-editor"
